{
  "name":"Replacer",
  "version": "1.0",
  "description": "Replace words",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ],
  "browser_action":{
    "default_popup": "popup/popup.html"
  }
}
